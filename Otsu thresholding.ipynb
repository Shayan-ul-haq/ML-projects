   "source": [
    "import cv2\n",
    "\n",
    "# Read image\n",
    "img = cv2.imread(\"Coins.png\")\n",
    "gray = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)\n",
    "\n",
    "# Otsu thresholding\n",
    "_, otsu = cv2.threshold(gray, 0, 255, cv2.THRESH_BINARY + cv2.THRESH_OTSU)\n",
    "\n",
    "cv2.imshow(\"Otsu Threshold\", otsu)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n"
   ]
